{
  "compilerOptions": {
      "baseUrl": ".",
      "paths": { "*": ["types/*"] },
    "target": "ES2016",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./dist/",
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "declaration": true,
    "types": [
      "node",
      "jasmine"
    ],
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}